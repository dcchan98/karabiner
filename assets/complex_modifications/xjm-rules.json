{
  "rules": [
    {
      "description": "[xjm] jk simultaneous to esc",
      "manipulators": [
        {
          "from": {
            "simultaneous": [
              {
                "key_code": "j"
              },
              {
                "key_code": "k"
              }
            ]
          },
          "to": [
            {
              "key_code": "escape"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "[xjm] ctrl+h -> cmd+{",
      "manipulators": [
        {
          "from": {
            "key_code": "h",
            "modifiers": {
              "mandatory": [
                "control"
              ]
            }
          },
          "to": [
            {
              "key_code": "open_bracket",
              "modifiers": [
                "command",
                "shift"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "[xjm] ctrl+l -> cmd+{",
      "manipulators": [
        {
          "from": {
            "key_code": "l",
            "modifiers": {
              "mandatory": [
                "control"
              ]
            }
          },
          "to": [
            {
              "key_code": "close_bracket",
              "modifiers": [
                "command",
                "shift"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    }
  ],
  "title": "xjm rules"
