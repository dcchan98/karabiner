{
    "rules": [
        {
            "description": "SpaceFN: Space enables SpaceFN mode (see: https://geekhack.org/index.php?topic=51069.0)",
            "manipulators": [
                {
                    "from": {
                        "key_code": "right_command",
                        "modifiers": {
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "set_variable": {
                                "name": "spacefn_mode",
                                "value": 1
                            }
                        }
                    ],
                    "to_after_key_up": [
                        {
                            "set_variable": {
                                "name": "spacefn_mode",
                                "value": 0
                            }
                        }
                    ],
                    "to_if_alone": [
                        {
                            "key_code": "right_command"
                        }
                    ],
                    "type": "basic"
                }
            ]
        },
        {
            "description": "Space+. -> Ctrl+space (change input source)",
            "manipulators": [
                {
                    "conditions": [
                        {
                            "name": "spacefn_mode",
                            "type": "variable_if",
                            "value": 1
                        }
                    ],
                    "from": {
                        "key_code": "period",
                        "modifiers": {
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "spacebar",
                            "modifiers": [
                                "control"
                            ]
                        }
                    ],
                    "type": "basic"
                }
            ]
        },
        {
